{
  "version": 2,
  "builds": [
    {
      "src": "Backend/dist/server.js", 
      "use": "@vercel/node",
      "config": {  
        "includeFiles": ["Backend/dist/**", "Backend/node_modules/.prisma/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Backend/dist/server.js"
    }
  ]
}